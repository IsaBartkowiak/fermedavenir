<div class="page-header">
  <%= link_to plantations_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_plantation_path(@plantation), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show plantation</h1>
</div>

<dl class="dl-horizontal">
  <dt>Farm:</dt>
  <dd><%= @plantation.farm %></dd>

  <dt>Generation:</dt>
  <dd><%= @plantation.generation %></dd>

  <dt>Quantity:</dt>
  <dd><%= @plantation.quantity %></dd>

  <dt>Year:</dt>
  <dd><%= @plantation.year %></dd>

</dl>
