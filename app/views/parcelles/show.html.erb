<div class="page-header">
  <%= link_to parcelles_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_parcelle_path(@parcelle), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show parcelle</h1>
</div>

<dl class="dl-horizontal">
  <dt>Location:</dt>
  <dd><%= @parcelle.location %></dd>

  <dt>Generation:</dt>
  <dd><%= @parcelle.generation %></dd>

</dl>
