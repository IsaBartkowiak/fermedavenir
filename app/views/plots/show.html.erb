<div class="page-header">
  <%= link_to plots_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_plot_path(@plot), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show plot</h1>
</div>

<dl class="dl-horizontal">
  <dt>Location:</dt>
  <dd><%= @plot.location %></dd>

  <dt>Generation:</dt>
  <dd><%= @plot.generation %></dd>

</dl>
