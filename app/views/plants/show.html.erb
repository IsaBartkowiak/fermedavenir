<div class="page-header">
  <%= link_to plants_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_plant_path(@plant), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show plant</h1>
</div>

<dl class="dl-horizontal">
  <dt>Titre:</dt>
  <dd><%= @plant.titre %></dd>

  <dt>Variete:</dt>
  <dd><%= @plant.variete %></dd>

  <dt>Type:</dt>
  <dd><%= @plant.caracteristique %></dd>
</dl>
