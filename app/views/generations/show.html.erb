<div class="section_grey">
<div class="page-header">
  <%= link_to generations_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_generation_path(@generation), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show generation</h1>
</div>

<dl class="dl-horizontal">
  <dt>Semi from:</dt>
  <dd><%= @generation.seedling_from %></dd>

  <dt>Plantation:</dt>
  <dd><%= @generation.plantation %></dd>

  <dt>Recolte:</dt>
  <dd><%= @generation.harvest %></dd>

  <dt>Conservation to:</dt>
  <dd><%= @generation.available_for %></dd>

</dl>
</div>