<div class="page-header">
  <%= link_to legumes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_legume_path(@legume), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show legume</h1>
</div>

<dl class="dl-horizontal">
  <dt>Titre:</dt>
  <dd><%= @legume.titre %></dd>

  <dt>Variete:</dt>
  <dd><%= @legume.variete %></dd>

  <dt>Type:</dt>
  <dd><%= @legume.caracteristique %></dd>
</dl>
